<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
  <form-beans>
    <form-bean name="com.mycompany.controledenotas.system.LogonActionForm" type="com.mycompany.controledenotas.system.LogonActionForm"/>
    <form-bean name="com.mycompany.controledenotas.system.MaintainRoleActionForm" type="com.mycompany.controledenotas.system.MaintainRoleActionForm"/>
    <form-bean name="com.mycompany.controledenotas.system.MaintainUserActionForm" type="com.mycompany.controledenotas.system.MaintainUserActionForm"/>
  </form-beans>
  <global-forwards type="org.apache.struts.action.ActionForward">
      <forward name="error" path="/infra/ErrorPage.jsp"/>
      <forward name="logon" path="/system/LogonForm.jsp"/>
      <forward name="menu" path="/system/HomePage.jsp"/>
      <forward name="index" path="/index.jsp"/>
  </global-forwards>
  <action-mappings type="org.apache.struts.action.ActionMapping">
    <action path="/system/LogonForm"
            type="com.mycompany.controledenotas.system.LogonAction"
            name="com.mycompany.controledenotas.system.LogonActionForm"
            input="/system/LogonForm.jsp"
            validate="true" scope="session">
      <forward name="same" path="/system/LogonForm.jsp"/>
    </action>
    <action path="/system/MaintainRoleForm"
            type="com.mycompany.controledenotas.system.MaintainRoleAction"
            name="com.mycompany.controledenotas.system.MaintainRoleActionForm"
            input="/system/MaintainRoleForm.jsp"
            validate="true" scope="session">
      <forward name="same" path="/system/MaintainRoleForm.jsp"/>
    </action>
    <action path="/system/MaintainUserForm"
            type="com.mycompany.controledenotas.system.MaintainUserAction"
            name="com.mycompany.controledenotas.system.MaintainUserActionForm"
            input="/system/MaintainUserForm.jsp"
            validate="true" scope="session">
      <forward name="same" path="/system/MaintainUserForm.jsp"/>
    </action>
  </action-mappings>
  <message-resources null="false" parameter="ApplicationResources"/>
  <message-resources key="ApplicationResources" null="false" parameter="ApplicationResources"/>
  <message-resources key="ErrorResources" null="false" parameter="ErrorResources"/>
</struts-config>
